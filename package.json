{
  "name": "multi-cloud-uploader",
  "version": "1.0.2",
  "description": "支持多云存储平台的文件上传，当前支持阿里云 OSS 和腾讯云 COS，未来将扩展到更多云服务。",
  "main": "./src/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "uploaderOSS:tice": "multi-cloud-uploader --uploadFrom=src --uploadTo=test/sailing  --ossConfig=./oss.tice.conf.json",
    "uploaderCOS:tice": "multi-cloud-uploader --uploadFrom=src --uploadTo=test/sailing  --cosConfig=./cos.tice.conf.json",
    "uploader:tice": "multi-cloud-uploader --uploadFrom=src --uploadTo=test/sailing  --cosConfig=./cos.tice.conf.json --ossConfig=./oss.tice.conf.json"
  },
  "keywords": [
    "upload",
    "cloud",
    "multi-cloud",
    "oss",
    "cos",
    "cloud-storage",
    "file-upload"
  ],
  "author": "Sailing",
  "license": "MIT",
  "bin": {
    "multi-cloud-uploader": "src/main.js"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "dependencies": {
    "ali-oss": "^6.17.1",
    "cos-nodejs-sdk-v5": "^2.14.6",
    "minimist": "^1.2.6"
  }
}
